        <script type="text/javascript">
            //<![CDATA[
            var DHTML = (document.getElementById || document.all || document.layers);
            var timeoutProgressBar;
            function ap_getObj(name)
            {
                if (document.getElementById)
                {
                    return document.getElementById(name).style;
                }
                else if (document.all)
                {
                    return document.all[name].style;
                }
                else if (document.layers)
                {
                    return document.layers[name];
                }
            }
            function ap_showWaitMessage(div, flag)
            {
                if (flag === 1) {
                    document.getElementById('waitDiv').style.top = waitDivPosition() + "px";
                    timeoutProgressBar = setTimeout("document.getElementById('waitDiv').style.visibility='visible';", 500);
                }
                if (flag === 0) {
                    clearTimeout(timeoutProgressBar);
                    document.getElementById('waitDiv').style.visibility = 'hidden';
                }
            }
            function waitDivPosition() {
                var divPosition;
                var heightPage;
                var scrollPosition;
                var sizeDiv = 80;
                heightPage = document.documentElement.clientHeight;
                scrollPosition = document.documentElement.scrollTop;
                divPosition = heightPage / 2 + scrollPosition - sizeDiv;
                return divPosition;
            }
            ap_showWaitMessage('waitDiv', 1);
            //]]>
