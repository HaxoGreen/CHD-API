<script type="text/javascript">
    function rollPage(howMuch)
    {
        jQuery('#pageNumber').val(parseInt(jQuery('#pageNumber').val()) + howMuch);
        doSubmit();
    }

    function changeSortDirection(newSortDirection)
    {
        jQuery('#sortDirection').val(newSortDirection);
        doSubmit();
    }

    function checkPageAndSubmit()
    {
        if (checkPageNumber())
            doSubmit();
    }

    function checkPageNumber()
    {
        jQFieldNumPage = jQuery('#pageNumber');
        if (jQFieldNumPage.length)
        {
            numPage = parseInt(jQFieldNumPage.val());
            if (isNaN(numPage) || (numPage > 47) || (numPage < 1))
            {
